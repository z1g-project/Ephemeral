{
	"name": "ephemeral",
	"private": true,
	"version": "0.7.0",
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "concurrently \"vite\" \"tsx index.ts\"",
		"prod": "vite build && tsx index.ts",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
